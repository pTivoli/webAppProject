<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
    <form-beans>
        <form-bean name="loginForm" type="it.pointPharma.formBeans.UserData"/>
    </form-beans>
    <action-mappings>
        <action input="/index.jsp" name="loginForm" path="/login" type="it.pointPharma.beans.LoginFormAction">
            <forward name="error" path="/index.jsp" /> <!--it must be changed?-->
            <forward name="REG" path="/JSP/REG.jsp" />
            <forward name="PM" path="/JSP/PM.jsp" />
            <forward name="PD" path="/JSP/PD.jsp" />
            <forward name="DO" path="/JSP/DO.jsp" />
        </action>
    </action-mappings>

</struts-config>